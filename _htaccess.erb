RewriteEngine on

RewriteRule wordpress/photos/album/(.*) /ee/i/simIndigo/album/$1 [R,L]
RewriteRule wordpress/photos/tags/(.*) /ee/i/simIndigo/flickr/tags/$1 [R,L]
RewriteRule wordpress/tags/(.*) /ee/i/simIndigo/flickr/tags/$1 [R,L]
RewriteRule wordpress/\d+/\d+/\d+/(.*)/ /ee/i/simIndigo/article/$1 [R,L]
RewriteRule wordpress/2007/01/14/個人戦-usa-yoo-versus-japan-hojo/ /ee/i/simIndigo/article/usa_yoo_versus_japan_hojo/ [R,L]

RewriteCond %{REQUEST_URI} !=/feed/
RewriteCond %{REQUEST_URI} !=/feed/index.html
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME}/index.html !-f
RewriteRule ^(.*)$ http://localhost:<%= port || '4242' %>/$1 [P,L]

AddHandler php5-script .php
DirectoryIndex index.html index.xml index.php
ErrorDocument 404 /404.html
